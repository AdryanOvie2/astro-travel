<style>
	.services {
		width: 40%;
		margin: auto;
		background-color: #fff;
		border-radius: 5px;
		border-color: white;
		margin-top: -2.3rem;
		box-shadow: 1px 1px 40px lightgray;

		display: grid;
		grid-template-rows: repeat(auto-fit, 1fr);
		grid-template-columns: repeat(4, 1fr);

